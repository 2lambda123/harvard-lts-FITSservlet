<?xml version="1.0" encoding="UTF-8"?>
<jboss-deployment-structure>
    <!--
        JBoss 7.1.1 - this file references a "module" in the JBoss installation
        which contains a module.xml file and points to the FITS file system.
    -->
	<deployment>
		<dependencies>
            <module name="fits" />
		</dependencies>
	</deployment>
</jboss-deployment-structure>